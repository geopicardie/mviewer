<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application title="Test Sureté dans les EPLE" logo="logoHDF.png" help="demo/demo_fonds_help.html" showhelp="false" exportpng="true" style="css/themes/chambray.css"/>
    <mapoptions maxzoom="19" projection="EPSG:3857" center="270000,6450000" zoom="8" projextent="145518,6726671,372058,6868691" />
    <baselayers style="gallery">
        <baselayer
            type="OSM"
            id="esriworldimagery"
            label="Esri world imagery"
            title="esriworldimagery"
            thumbgallery="img/basemap/ortho.jpg"
            url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
            visible="true"
             maxzoom="19"
           attribution="Esri world imagery"/>
        <baselayer
            type="WMS"
            id="osm-geopic-bright-wms"
            label="OSM - GéoPicardie bright WMS"
            title="OpenStreetMap - fond GéoPicardie contrasté"
            thumbgallery="img/basemap/osm.geopicardie.bright.png"
            url="http://osm.geopicardie.fr/mapproxy/service"
            layers="bright"
            format="image/png"
            tiled="false"
            maxzoom="19"
            attribution="Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'&gt;OpenStreetMap &lt;/a&gt;,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a&gt;"
            visible="false"/>
        <!--baselayer
            type="OSM"
            id="darkmatter"
            label="Dark Matter"
            title="CartoDb"
            thumbgallery="img/basemap/darkmatter.png"
            url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png"
            maxzoom="19"
            visible="false"
            attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" /-->
        <baselayer
            type="OSM"
            id="positron"
            label="Positron"
            title="CartoDb"
            thumbgallery="img/basemap/positron.png"
            url="http://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"
            maxzoom="19"
            visible="false"
            attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
    </baselayers>
    <proxy url=""/>
    <olscompletion url="http://api-adresse.data.gouv.fr/search/" type="ban" attribution="La recherche d'adresse est un service proposé par la BAN"/>   
    <themes mini="true">
        <theme name="Etablissements"  id="etablissements" collapsed="true" icon="graduation-cap">
            <layer id="lycees" name="Lycée" visible="true" tiled="false"
                fields="NOM_ETAB,ADRESSE,CODE_POSTA,COMMUNE" aliases="Nom, Adresse,Code postal,Commune"
                queryable="true"
                searchable="true"
                localsearch="true" localsearch_keys="NOM_ETAB" 
                infoformat="text/html" featurecount="1"
                style="eple_surete" type="geojson" url="data/lycees_hdf_pt_surete.geojson" 
                attribution="Région Hauts-de-France" 
                metadata="http://www.geopicardie.fr/geonetwork/apps/georchestra/?uuid=b13e50c5-518d-40f4-abd6-e33be37b20c3" 
                metadata-csw="http://www.geopicardie.fr/geonetwork/srv/eng/csw?SERVICE=CSW&amp;VERSION=2.0.2&amp;REQUEST=GetRecordById&amp;elementSetName=full&amp;ID=b13e50c5-518d-40f4-abd6-e33be37b20c3" />     
            <layer id="lycees_emprise" name="Emprise lycée" visible="true" tiled="false"
                fields="NOM_ETAB,ADRESSE,CODE_POSTA,COMMUNE" aliases="Nom, Adresse,Code postal,Commune"
                queryable="true"
                infoformat="text/html" featurecount="1"
                style="eple_surete_emprise" type="geojson" url="data/lycees_hdf_emprise_surete.geojson" 
                attribution="Région Hauts-de-France" 
                metadata="http://www.geopicardie.fr/geonetwork/apps/georchestra/?uuid=b13e50c5-518d-40f4-abd6-e33be37b20c3" 
                metadata-csw="http://www.geopicardie.fr/geonetwork/srv/eng/csw?SERVICE=CSW&amp;VERSION=2.0.2&amp;REQUEST=GetRecordById&amp;elementSetName=full&amp;ID=b13e50c5-518d-40f4-abd6-e33be37b20c3" />
        </theme>
    </themes>
</config>
